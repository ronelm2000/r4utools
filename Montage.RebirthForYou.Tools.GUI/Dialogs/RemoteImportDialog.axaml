<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" 
        d:DesignWidth="300"
        Width="400"
        ShowInTaskbar="false"
        WindowStartupLocation="CenterOwner"
        CanResize="false"
        x:Class="Montage.RebirthForYou.Tools.GUI.Dialogs.RemoteImportDialog"
        Title="Import Deck..."
        SizeToContent="Height"
        >
  <DockPanel>
    <Border Padding="4">
      <StackPanel>
        <TextBox  Name="importURLTextBox" Watermark="URL to Import..." />
        <Button Name="importButton">Import</Button>
      </StackPanel>
    </Border>
  </DockPanel>
</Window>
